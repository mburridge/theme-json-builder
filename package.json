{
  "name": "theme-json-builder",
  "version": "0.0.1",
  "description": "A WordPress plugin to help generate theme.json files",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && composer install",
    "start": "wp-scripts start",
    "build": "wp-scripts build",
		"env": "wp-env",
		"env:start": "wp-env start",
		"env:stop": "wp-env stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanwelcher/theme-json-builder.git"
  },
  "keywords": [
    "WordPress",
    "Gutenberg",
    "Full Site Editing"
  ],
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/ryanwelcher/theme-json-builder/issues"
  },
  "homepage": "https://github.com/ryanwelcher/theme-json-builder#readme",
  "devDependencies": {
    "@wordpress/env": "^4.1.3",
    "@wordpress/scripts": "^19.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6"
  },
  "lint-staged": {
    "*.php": [
      "composer run lint"
    ],
    "*.css": [
      "wp-scripts lint-style"
    ],
    "*.{js}": [
      "wp-scripts lint-js"
    ]
  },
  "dependencies": {
    "@wordpress/dom-ready": "^3.2.2",
    "@wordpress/element": "^4.0.3"
  }
}
